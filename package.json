{
  "name": "podverse",
  "version": "3.1.11",
  "private": true,
  "contributors": [
    "Creon Creonopoulos",
    "Mitch Downey",
    "Gary Johnson",
    "Kyle Downey"
  ],
  "scripts": {
    "dev": "react-native start",
    "dev:android": "react-native run-android",
    "dev:ios": "react-native run-ios --simulator 'iPhone 12 Pro Max'",
    "dev:ios:device": "react-native run-ios --device",
    "lint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "format": "prettier --write src/**/*.ts*",
    "jetify": "jetify",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "stage:resetDatabase": "node ./scripts/_resetDatabase.js",
    "test:e2e:android": "export $(cat .env | xargs) && DEVICE_TYPE=Android BROWSERSTACK_DEVICE='Google Pixel 3' BROWSERSTACK_OS_VERSION='9.0' node ./functions/tests/e2e/index.js",
    "test:w:e2e:android": "BROWSERSTACK_TEST_ENV='local' DEVICE_TYPE='Android' node ./functions/tests/e2e/index.js",
    "test:e2e:fdroid": "export $(cat .env | xargs) && DEVICE_TYPE=F-Droid BROWSERSTACK_DEVICE='Google Pixel 3' BROWSERSTACK_OS_VERSION='9.0' node ./functions/tests/e2e/index.js",
    "test:w:e2e:fdroid": "BROWSERSTACK_TEST_ENV='local' DEVICE_TYPE='F-Droid' node ./functions/tests/e2e/index.js",
    "test:e2e:ios": "export $(cat .env | xargs) && DEVICE_TYPE=iOS BROWSERSTACK_DEVICE='iPhone 12 Pro Max' BROWSERSTACK_OS_VERSION='13.0' node ./functions/tests/e2e/index.js",
    "postinstall": "patch-package",
    "version_update": "babel-node ./scripts/update_version.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.12.13",
    "@podverse/podcast-feed-parser": "^1.0.14",
    "@react-native-community/async-storage": "1.12.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "6.0.0",
