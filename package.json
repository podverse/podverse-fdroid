{
  "name": "podverse",
  "version": "2.10.2",
  "private": true,
  "contributors": [
    "Creon Creonopoulos",
    "Mitch Downey",
    "Gary Johnson"
  ],
  "scripts": {
    "dev": "react-native start",
    "dev:android": "react-native run-android",
    "dev:ios": "react-native run-ios --simulator 'iPhone 12 Pro Max'",
    "format": "prettier --write src/**/*.ts*",
    "jetify": "jetify",
    "lint": "tslint --fix 'src/**/*.{ts,tsx}'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "stage:resetDatabase": "node ./scripts/_resetDatabase.js",
    "test:e2e:android": "export $(cat .env | xargs) && DEVICE_TYPE=Android BROWSERSTACK_DEVICE='Google Pixel 3' BROWSERSTACK_OS_VERSION='9.0' node ./functions/tests/e2e/index.js",
    "test:w:e2e:android": "BROWSERSTACK_TEST_ENV='local' DEVICE_TYPE='Android' node ./functions/tests/e2e/index.js",
    "test:e2e:fdroid": "export $(cat .env | xargs) && DEVICE_TYPE=F-Droid BROWSERSTACK_DEVICE='Google Pixel 3' BROWSERSTACK_OS_VERSION='9.0' node ./functions/tests/e2e/index.js",
    "test:w:e2e:fdroid": "BROWSERSTACK_TEST_ENV='local' DEVICE_TYPE='F-Droid' node ./functions/tests/e2e/index.js",
    "test:e2e:ios": "export $(cat .env | xargs) && DEVICE_TYPE=iOS BROWSERSTACK_DEVICE='iPhone 12 Pro Max' BROWSERSTACK_OS_VERSION='13.0' node ./functions/tests/e2e/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "tslint",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/helper-validator-identifier": "7.10.4",
    "@podverse/podcast-feed-parser": "^1.0.11",
    "@react-native-community/async-storage": "1.12.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "4.7.0",
    "@react-native-community/picker": "1.8.0",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "@types/lodash.memoize": "4.1.6",
    "@types/react-native-background-timer": "2.0.0",
    "@types/react-native-rss-parser": "1.4.0",
    "@types/validator": "12.0.1",
    "axios": "0.21.1",
    "bad-words": "3.0.3",
    "bottleneck": "2.19.5",
    "eventemitter3": "4.0.7",
    "he": "1.2.0",
    "linkifyjs": "2.1.9",
    "lodash": "4.17.20",
    "lodash.memoize": "4.1.2",
    "podverse-shared": "^1.0.3",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-background-downloader": "2.3.4",
    "react-native-background-timer": "2.4.0",
    "react-native-cheerio": "1.0.0-rc.4",
    "react-native-config": "1.0.0",
    "react-native-dialog": "5.6.0",
    "react-native-elements": "1.2.7",
    "react-native-fast-image": "7.0.2",
    "react-native-fs": "2.16.6",
    "react-native-gesture-handler": "1.8.0",
    "react-native-iap": "4.6.3",
    "react-native-localize": "1.4.1",
    "react-native-picker-select": "6.6.0",
    "react-native-reanimated": "1.13.1",
    "react-native-render-html": "4.2.4",
    "react-native-safe-area-context": "3.1.8",
    "react-native-safe-area-view": "^2.0.0",
    "react-native-screens": "2.11.0",
    "react-native-secure-key-store": "2.0.9",
    "react-native-share": "3.8.5",
    "react-native-sortable-list": "0.0.24",
    "react-native-swipe-list-view": "2.5.0",
    "react-native-system-setting": "1.7.5",
    "react-native-track-player": "1.2.3",
    "react-native-vector-icons": "6.7.0",
    "react-native-webview": "11.0.0",
    "react-navigation": "4.4.1",
    "react-navigation-drawer": "1.4.0",
    "react-navigation-stack": "1.7.3",
    "react-navigation-tabs": "1.2.0",
    "reactn": "2.2.7",
    "stream": "^0.0.2",
    "timers": "^0.1.1",
    "validator": "12.2.0",
    "wd": "1.13.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/runtime": "7.11.2",
    "@types/axios": "0.14.0",
    "@types/he": "1.1.1",
    "@types/linkifyjs": "2.1.3",
    "@types/lodash": "4.14.161",
    "@types/react": "16.9.49",
    "@types/react-native": "0.63.23",
    "@types/react-native-dialog": "5.6.2",
    "@types/react-native-share": "3.3.0",
    "@types/react-native-sortable-list": "0.0.9",
    "@types/react-native-vector-icons": "6.4.6",
    "@types/react-navigation": "3.4.0",
    "dotenv": "8.2.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "metro-react-native-babel-preset": "0.63.0",
    "prettier": "1.19.1",
    "react-native-typescript-transformer": "1.2.13",
    "react-test-renderer": "16.13.1",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-plugin-prettier": "2.3.0",
    "tslint-react": "3.6.0",
    "typescript": "3.9.7"
  },
  "resolutions": {
    "react-native-safe-area-view": "^2.0.0"
  },
  "isSwift": true
}
